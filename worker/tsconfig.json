{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "lib": ["ES2020"],
      "types": ["@cloudflare/workers-types"],
      "moduleResolution": "bundler",
      "strict": true,
      "noEmit": false,
      "esModuleInterop": true,
      "baseUrl": ".",
      "outDir": "./dist",
      "skipLibCheck": true,
      "allowJs": true,
      "checkJs": false
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }